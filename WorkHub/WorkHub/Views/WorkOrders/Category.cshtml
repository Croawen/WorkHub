<!DOCTYPE html>
<div class="row">
    @foreach (var item in ViewBag.WorkOrders)
    {
        <div class="col s2">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">@item.Payment $</span>
                    <p>
                        @item.Description
                    </p>
                </div>
                <div class="card-action">
                    <a href="/WorkOrders/Details/@item.Id">Details</a>
                </div>
            </div>
        </div>
    }
</div>